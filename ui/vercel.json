{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "rewrites": [
    { "source": "/proxy/api/:path*", "destination": "http://34.78.228.202/api/:path*" },
    { "source": "/proxy/agent/:path*", "destination": "http://34.78.228.202/agent/:path*" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
